# GitHub webhook secret. Used to verify incoming webhook payloads from GitHub.
WEBHOOK_SECRET=

# GitHub App private key (PEM format, single line, no line breaks).
GH_APP_PK=

# GitHub App ID. Found in your GitHub App settings.
GH_APP_ID=

# GitHub App Client ID. Used for OAuth flows.
GH_CLIENT_ID=

# Redis connection string. Default connects to the local Redis service in Docker Compose.
REDIS_URL=redis://redis:6379/0 